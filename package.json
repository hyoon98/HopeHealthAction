{
  "name": "cbr_manager",
  "version": "1.0.0",
  "description": "Jupiter CBR_Manager",
  "main": "app.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon app.js",
    "client": "npm start --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:reset": "npx sequelize-cli db:drop & npx sequelize-cli db:create & npx sequelize-cli db:migrate && npx sequelize-cli db:seed --seed /seeders/demo2",
    "test:server": "cross-env NODE_ENV=test mocha tests/server"
  },
  "repository": {
    "type": "git",
    "url": "https://csil-git1.cs.surrey.sfu.ca/373-2021-1-Jupiter/cbr_manager.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nivo/bar": "^0.67.0",
    "@nivo/core": "^0.67.0",
    "@nivo/line": "^0.67.0",
    "@nivo/pie": "^0.67.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "excel4node": "^1.7.2",
    "express": "^4.17.1",
    "faker": "^5.4.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "nodemon": "^2.0.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "react-google-maps": "^9.4.5",
    "react-icons": "^4.2.0",
    "react-modal": "^3.12.1",
    "react-paginate": "^7.1.0",
    "react-router-dom": "^5.2.0",
    "reactstrap-date-picker": "^0.0.11",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.6"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "mocha": "^8.3.2",
    "supertest": "^6.1.3"
  }
}
